<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>M3USharp</AssemblyName>
    <RootNamespace>M3USharp</RootNamespace>
    <LangVersion>8</LangVersion>
    <Authors>Frederik Riboe Olsen</Authors>
    <Company />
    <PackageTags>m3u m3u8 hls</PackageTags>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/Riboe/M3USharp</RepositoryUrl>
